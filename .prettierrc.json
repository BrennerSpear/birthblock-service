{
    "semi": true,
    "tabWidth": 4,
    "printWidth": 100,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@api/(.*)$",
        "^@providers/(.*)$",
        "^@components/(.*)$",
        "^@utils(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true
}
